<% include ./partials/header %>
<h1> Home </h1>
<% images.forEach(function(image) { %>
  <div>
    <div class="">
      <h2><%=image.user_id%></h2>
    </div>
    <div class="">
        <img src="<%=image.src%>">
    </div>
    <div class="">
      <span> <%=image.tag_id %></span>
    </div>
    <div class="">
      <a href="/image/<%=image.id%>">View</a>
    </div>
  </div>
<% }) %>

<% include ./partials/footer %>
